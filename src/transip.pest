jaja = { SOI ~ vps_command ~ EOI }
name = { ASCII_ALPHANUMERIC+ }
description = { ASCII_ALPHANUMERIC+ }
space = _{ " " }
vps_list = { "list" ~ space* }
vps_item_action = { ( "item" | "reset" | "lock" | "unlock" | "start" | "stop" ) ~ space+ ~ name ~ space* }
vps_command = { "vps" ~ space+ ~ ( vps_list | vps_item_action ) }
